{
  "name": "Pilota - Python Scraper",
  "dockerFile": "../Dockerfile",
  "customizations": {
    "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-azuretools.vscode-docker"
